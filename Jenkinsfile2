pipeline {
    agent any
    tools {
        maven 'localmvn'
    }
    stages {
        stage('Build') {
            inut {
                message 'do you hungry?'
                ok 'Yes, I am'
                submiter 'lex,john'
                parameters {
                    string(name: 'PERSON', defaultValue: 'Jenkins',
                           description: 'Description about dessision') 
                           }
                }
            }
            steps {
                echo 'Hello, ${PERSON}'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                build job: 'checkstyle'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
